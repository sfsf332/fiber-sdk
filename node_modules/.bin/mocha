#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/mocha@11.1.0/node_modules/mocha/bin/node_modules:/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/mocha@11.1.0/node_modules/mocha/node_modules:/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/mocha@11.1.0/node_modules:/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/mocha@11.1.0/node_modules/mocha/bin/node_modules:/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/mocha@11.1.0/node_modules/mocha/node_modules:/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/mocha@11.1.0/node_modules:/Users/jack/Documents/fiber-sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mocha/bin/mocha.js" "$@"
else
  exec node  "$basedir/../mocha/bin/mocha.js" "$@"
fi
